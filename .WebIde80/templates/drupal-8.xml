<templateSet group="Drupal 8">
  <template name="FieldDefinition::create" value="$fields['$name$'] = FieldDefinition::create('$type$')&#10;  -&gt;setLabel(t('$label$'))&#10;  -&gt;setDescription(t('$description$'))&#10;  -&gt;setReadOnly($readOnly$)&#10;  -&gt;setSettings('$settingsName$', $settingsValue$);&#10;&#10;FieldDefinition::create$END$" description="D8" toReformat="false" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="&quot;my_field&quot;" alwaysStopAt="true" />
    <variable name="type" expression="enum(&quot;integer&quot;, &quot;string&quot;, &quot;language&quot;, &quot;entity_reference&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="label" expression="capitalize(underscoresToSpaces(name))" defaultValue="" alwaysStopAt="true" />
    <variable name="description" expression="" defaultValue="&quot;Useful description&quot;" alwaysStopAt="true" />
    <variable name="readOnly" expression="enum(&quot;FALSE&quot;, &quot;TRUE&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="settingsName" expression="enum(&quot;unsigned&quot;, &quot;target_type&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="settingsValue" expression="enum(&quot;TRUE&quot;, &quot;FALSE&quot;, &quot;'foo'&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="false" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="SQL" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="PHP" value="true" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="@ContentEntityType" value="@ContentEntityType(&#10; *   id = &quot;$id$&quot;,&#10; *   label = @Translation(&quot;$label$&quot;),&#10; *   bundle_entity_type = &quot;$id$_$bundle$&quot;,&#10; *   bundle_label = @Translation(&quot;$label$ type&quot;),&#10; *   controllers = {&#10; *     &quot;storage&quot;      = &quot;Drupal\$module$\$idCamel$Storage&quot;,&#10; *     &quot;access&quot;       = &quot;Drupal\$module$\$idCamel$AccessController&quot;,&#10; *     &quot;translation&quot;  = &quot;Drupal\$module$\$idCamel$TranslationHandler&quot;,&#10; *     &quot;view_builder&quot; = &quot;Drupal\$module$\$idCamel$ViewBuilder&quot;,&#10; *     &quot;list_builder&quot; = &quot;Drupal\$module$\$idCamel$ListBuilder&quot;,&#10; *     &quot;form&quot; = {&#10; *       &quot;default&quot; = &quot;Drupal\$module$\$idCamel$Form&quot;,&#10; *       &quot;edit&quot;    = &quot;Drupal\$module$\$idCamel$Form&quot;,&#10; *       &quot;delete&quot;  = &quot;Drupal\$module$\Form\$idCamel$DeleteConfirm&quot;&#10; *     }&#10; *   },&#10; *   base_table          = &quot;mystyle_subject_b_base&quot;,&#10; *   data_table          = &quot;mystyle_subject_b_data&quot;,&#10; *   revision_table      = &quot;mystyle_subject_r_base&quot;,&#10; *   revision_data_table = &quot;mystyle_subject_r_data&quot;,&#10; *   fieldable = TRUE,&#10; *   translatable = TRUE,&#10; *   entity_keys = {&#10; *     &quot;id&quot;       = &quot;id&quot;,&#10; *     &quot;revision&quot; = &quot;revision&quot;,&#10; *     &quot;bundle&quot;   = &quot;bundle&quot;,&#10; *     &quot;label&quot;    = &quot;label&quot;,&#10; *     &quot;uuid&quot;     = &quot;uuid&quot;&#10; *   },&#10; *   bundle_entity_type = &quot;mystyle_subject_bundle&quot;,&#10; *   links = {&#10; *     &quot;admin-form&quot;      = &quot;mystyle.subject_bundle_edit&quot;,&#10; *     &quot;canonical&quot;       = &quot;mystyle.subject_view&quot;,&#10; *     &quot;delete-form&quot;     = &quot;mystyle.subject_delete_confirm&quot;,&#10; *     &quot;edit-form&quot;       = &quot;mystyle.subject_edit&quot;,&#10; *     &quot;version-history&quot; = &quot;mystyle.subject_revision_overview&quot;&#10; *   }&#10; * )" description="Annotation for Content entity type" toReformat="false" toShortenFQNames="true">
    <variable name="id" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="label" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="bundle" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="module" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="idCamel" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="false" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="SQL" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="PHP" value="false" />
      <option name="PHP Comment" value="true" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="ra_details" value="array(&#10;  '#type' =&gt; 'details',&#10;  '#tree' =&gt; $tree$,&#10;  '#title' =&gt; t('$title$'),&#10;  '#open' =&gt; $open$,&#10;)$delimiter$" toReformat="false" toShortenFQNames="true">
    <variable name="tree" expression="enum(&quot;TRUE&quot;, &quot;FALSE&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="title" expression="" defaultValue="&quot;Foo&quot;" alwaysStopAt="true" />
    <variable name="open" expression="enum(&quot;TRUE&quot;, &quot;FALSE&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="delimiter" expression="enum(&quot;;&quot;, &quot;,&quot;, &quot;&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="dsmv" value="drupal_set_message(String::format('&lt;pre&gt;Line: @line $$$variable_name$ = @values&lt;/pre&gt;', array(&#10;  '@line' =&gt; __LINE__,&#10;  '@value' =&gt; print_r($$$variable_name$, TRUE),&#10;)));" description="D8 drupal_set_message - variable" toReformat="false" toShortenFQNames="true">
    <variable name="variable_name" expression="" defaultValue="&quot;my_var&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>